#!/bin/sh
#
# This script will be executed *after* all the other init scripts.
# You can put your own initialization stuff in here if you don't
# want to do the full Sys V style init stuff.

touch /var/lock/subsys/local

# for redis
echo never > /sys/kernel/mm/transparent_hugepage/enabled
ip route add default via 172.16.16.57 dev em1 src 172.16.16.58 table em1
ip rule add from 172.16.16.58 table em1
ip route add default via 172.16.16.61 dev em2 src 172.16.16.62 table em2
ip rule add from 172.16.16.62 table em2
